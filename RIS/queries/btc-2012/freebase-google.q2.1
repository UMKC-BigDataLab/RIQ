# Freebase
# Query 2.1
 
select ?filmPerformance ?actor ?film ?name ?initialReleaseDate
where
{
    ?filmPerformance <http://rdf.freebase.com/ns/film.performance.actor> ?actor .
    ?filmPerformance <http://rdf.freebase.com/ns/film.performance.film> ?film .
    ?film <http://rdf.freebase.com/ns/type.object.name> ?name .
    ?film <http://rdf.freebase.com/ns/film.film.initial_release_date> ?initialReleaseDate .
}
